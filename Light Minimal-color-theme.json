    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#9098a0",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#70a880"
            }
        },
        {
            "name": "Regular Expressions",
            "scope": "string.regexp",
            "settings": {
                "foreground": "#70a0b0"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric",
                "constant.character.numeric"
            ],
            "settings": {
                "foreground": "#b09070"
            }
        },
        {
            "name": "Keywords & Storage",
            "scope": [
                "keyword",
                "storage",
                "storage.type"
            ],
            "settings": {
                "foreground": "#7090b0"
            }
        },
        {
            "name": "Functions & Methods",
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#b0a070"
            }
        },
        {
            "name": "Classes & Types",
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class",
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#70a8a0"
            }
        },
        {
            "name": "Variables & Parameters",
            "scope": [
                "variable",
                "variable.parameter",
                "variable.other"
            ],
            "settings": {
                "foreground": "#808090"
            }
        },
        {
            "name": "Constants",
            "scope": [
                "constant",
                "variable.other.constant"
            ],
            "settings": {
                "foreground": "#a080a0"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator",
                "punctuation"
            ],
            "settings": {
                "foreground": "#a0a8b0"
            }
        },
        {
            "name": "Tags (HTML/XML)",
            "scope": [
                "entity.name.tag",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#7090b0"
            }
        },
        {
            "name": "Attributes",
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#b0a070"
            }
        },
        {
            "name": "CSS Properties",
            "scope": "support.type.property-name.css",
            "settings": {
                "foreground": "#70a8a0"
            }
        },
        {
            "name": "CSS Values",
            "scope": "support.constant.property-value.css",
            "settings": {
                "foreground": "#b09070"
            }
        },
        {
            "name": "JSON Keys",
            "scope": "support.type.property-name.json",
            "settings": {
                "foreground": "#7090b0"
            }
        },
        {
            "name": "Markdown Headers",
            "scope": [
                "markup.heading",
                "entity.name.section"
            ],
            "settings": {
                "foreground": "#7090b0",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Links",
            "scope": [
                "markup.underline.link",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#70a880"
            }
        },
        {
            "name": "Markdown Bold",
            "scope": "markup.bold",
            "settings": {
                "foreground": "#b0a070",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Italic",
            "scope": "markup.italic",
            "settings": {
                "foreground": "#70a0b0",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "foreground": "#c87070"
            }
        },
        {
            "name": "Deprecated",
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#a86060",
                "fontStyle": "italic"
            }
        }
    ]
}